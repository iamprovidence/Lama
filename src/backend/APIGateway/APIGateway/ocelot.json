{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5001"
  },
  "ReRoutes": [
    {
      "SwaggerKey": "photo",
      "UpstreamPathTemplate": "/api/photo",
      "DownstreamPathTemplate": "/api/photo",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7001
        }
      ]
    },
    {
      "SwaggerKey": "lama",
      "UpstreamPathTemplate": "/api/{everything}",
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6001
        }
      ]
    }
  ],

  "SwaggerEndPoints": [
    {
      "Key": "lama",
      "Config": [
        {
          "Name": "Lama API",
          "Version": "v1",
          "Url": "https://localhost:6001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "photo",
      "Config": [
        {
          "Name": "Photo API",
          "Version": "v1",
          "Url": "https://localhost:7001/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}